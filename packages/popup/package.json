{
	"name": "@phragon-react/popup",
	"version": "1.0.0",
	"author": "phragon@websoftlab.com",
	"license": "MIT",
	"scripts": {
		"build": "phragon-script build --cwd \"../../\" -P @phragon-react/popup",
		"prettier": "phragon-script prettier --cwd \"../../\" -P @phragon-react/popup"
	},
	"dependencies": {
